{
  "name": "invent3-frontend",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 turbo dev",
    "build:client": "turbo build --filter=client",
    "dev:client": "NODE_TLS_REJECT_UNAUTHORIZED=0 turbo dev --filter=client",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@commitlint/cz-commitlint": "^19.4.0",
    "@types/react": "19.1.5",
    "@types/react-dom": "19.1.5",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "commitizen": "^4.3.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.2.6",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.10",
    "prettier": "^3.2.5",
    "standard-version": "^9.5.0",
    "turbo": "^2.1.1",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@9.12.0",
  "engines": {
    "node": ">=18 <=20",
    "pnpm": ">=9.12.0"
  },
  "browser": {
    "crypto": false,
    "stream": false
  },
  "dependencies": {
    "next": "14.3.0-canary.87",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "pnpm": {
    "overrides": {
      "react": "19.1.0",
      "react-dom": "19.1.0",
      "@types/react": "19.1.5",
      "@types/react-dom": "19.1.5"
    }
  }
}